{
  "name": "Select Extension",
  "version": "0.0",
  "description": "Select extension by Agam360",
  "manifest_version":2,
  "background": {
    "scripts": ["background.js"]
    },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "js": ["script.js"]
    }
  ]
}